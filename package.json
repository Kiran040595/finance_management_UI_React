{
  "name": "finance-management-ui",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/x-data-grid": "^7.23.6",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.0.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "homepage": "./",
    "electron-start": "npm run build && electron .",
    "electron-dev": "electron .",
    "package": "electron-packager . Finance --platform=win32 --arch=x64 --out=dist --overwrite",
    "package-all": "electron-builder -mwl",
    "package-win": "electron-packager . Finance --platform=win32 --arch=x64 --out=dist --overwrite",
    "dist": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "electron-packager": "^17.1.2",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.15"
  },
  "build": {
    "appId": "com.finance.management",
    "productName": "FinanceManagement",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "extends": null,
    "asar": true,
    "asarUnpack": [
      "backend/finance-management-1.0.0.jar",
      "custom-jre/**/*"
    ],
    "files": [
      "**/*",
      "build/**/*",
      "main.js",
      "public/**/*",
      "backend/finance-management-1.0.0.jar",
      "custom-jre/**/*"
    ],
    "directories": {
      "output": "dist"
    }
  }
}
